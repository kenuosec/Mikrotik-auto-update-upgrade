:local Mail xxxxxxxxxxxxxxx;
/export file=backup;
:delay 5s;
/system backup save name=backup;
:delay 5s;
/tool e-mail send to=$Mail subject="BACKUP" body="" file=backup.backup;
:delay 5s;
/tool e-mail send to=$Mail subject="BACKUP" body="" file=backup.rsc;
:delay 10s;
/file remove backup.backup;
/file remove backup.rsc;
:delay 10s;
